  "description": "A minimal Javascript library that improves the way you work with localStorage or sessionStorage.",
  "main": "dist/webStorage.js",
  "scripts": {
    "build": "webpack --env=build",
    "dev": "webpack --progress --colors --env=dev",
    "watch": "webpack --progress --colors --watch --env=dev",
