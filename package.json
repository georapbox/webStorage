  "author": "George Raptis <georapbox@gmail.com> (georapbox.github.io)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/georapbox/webStorage/issues"
  },
  "homepage": "https://github.com/georapbox/webStorage#readme",
  "devDependencies": {
    "babel-cli": "~6.26.0",
    "babel-core": "~6.26.0",
    "babel-loader": "~7.1.2",
    "babel-plugin-add-module-exports": "~0.2.1",
    "babel-preset-env": "~1.6.0",
    "eslint": "~4.7.1",
    "eslint-loader": "~1.9.0",
    "jasmine-core": "~2.8.0",
    "karma": "~1.7.1",
    "karma-cli": "~1.0.1",
    "karma-jasmine": "~1.1.0",
    "karma-phantomjs-launcher": "~1.0.4",
    "phantomjs-prebuilt": "~2.1.15",
    "webpack": "~3.6.0",
    "yargs": "~9.0.1"
